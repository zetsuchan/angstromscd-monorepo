# Vector Provider Configuration
# Options: chroma, qdrant, pgvector
VECTOR_PROVIDER=chroma

# OpenAI API Key (required for all providers)
OPENAI_API_KEY=your_openai_key

# ChromaDB Configuration
CHROMA_URL=http://localhost:8000

# Qdrant Configuration
QDRANT_URL=http://localhost:6333
# Optional: Qdrant API key for authentication
# QDRANT_API_KEY=your-qdrant-api-key

# PostgreSQL with pgvector Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/angstromscd

# Service Port
PORT=3003
